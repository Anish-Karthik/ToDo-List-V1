<%- include('header') %>
<body>
    <div class = 'box' id = "heading">
        <h1 ><%= ListTitle %></h1>
    </div>
    <div class="box">
        <% for(var i = 0; i < taskList.length; i++) { %>
            <div class="item">
                <input type="checkbox">
                <p><%= taskList[i] %></p>
            </div>
        <% } %>
        <form action="<%= route %>" method="post">
            <input type="text" name="newTask" placeholder="New Task" autocomplete="off">
            <button type="submit">+</button>
        </form>
    </div>
</body>
<%- include('footer') %>